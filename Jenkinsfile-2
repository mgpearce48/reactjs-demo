pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'building...'
                sh 'docker ps -a'
            }
        }
        stage('Test') {
            steps {
                echo 'testing...'
                sh 'docker images'
            }
        }
        stage('Deliver') {
            steps {
                echo 'delivering...'
                sh 'docker --version'
            }
        }
    }
}
